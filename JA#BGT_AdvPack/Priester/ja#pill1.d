BEGIN ~JA#PILL1~

IF ~NumTimesTalkedTo(0)~ THEN BEGIN JA#PILL1_1
SAY @0
IF ~~ THEN REPLY @1 GOTO JA#PILL1_3
IF ~~ THEN REPLY @2 GOTO JA#PILL1_4
IF ~~ THEN REPLY @3 GOTO JA#PILL1_2
END

IF ~~ THEN BEGIN JA#PILL1_2
SAY @4
IF ~~ THEN EXIT
END

IF ~~ THEN BEGIN JA#PILL1_3
SAY @5
IF ~~ THEN DO ~StartStore("ja#ilma1",LastTalkedToBy(Myself))~ EXIT
END

IF ~~ THEN BEGIN JA#PILL1_4
SAY @6
IF ~~ THEN REPLY @7 GOTO JA#PILL1_3
IF ~~ THEN REPLY @3 GOTO JA#PILL1_2
END

IF ~NumTimesTalkedToGT(0)~ THEN BEGIN JA#PILL1_5
SAY @8
IF ~~ THEN REPLY @1 GOTO JA#PILL1_3
IF ~~ THEN REPLY @3 GOTO JA#PILL1_2
END